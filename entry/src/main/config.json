{
  "app": {
    "bundleName": "com.example.firstwearableproject",
    "vendor": "example",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "name": "default",
        "icon": "$media:app_icon",
        "label": "$string:labelString",
        "type": "page"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/genderInfo/genderInfo",
          "pages/ageInfo/ageInfo",
          "pages/heightInfo/heightInfo",
          "pages/weightInfo/weightInfo",
          "pages/heartRate/heartRate",
          "pages/resultPage/resultPage"
        ],
        "name": "default"
      }
    ]
  }
}